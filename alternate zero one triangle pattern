#include<iostream>
using namespace std;
int main()
{
    int i,j=1,k=0;
    for(i=1;i<=5;i++){
            i%2==0 ? k=0 : k=1;

        for(j=1;j<=6;j++)
             if(i==1)
                {
                    cout<<k;
                    break;
                   // k=1-k;
                }
            else if(j<=i+1)
            {
                cout<<k;
                //k=1-k;
                k==0 ? k++ : k--;
            }
            else
                    cout<<" ";


    cout<<endl;
    }
}
